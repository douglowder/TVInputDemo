+    start: (callback) => {
+      value.setValue(config.toValue);
+      callback && callback({ finished: true });
+    },
+  });
+
+  return RN;
+});
+
+const icons = ['magnify', 'camera', 'inbox', 'heart', 'shopping-music'];
+
+const createState = (index, length) => ({
+  index,
+  routes: Array.from({ length }, (_, i) => ({
+    key: `key-${i}`,
