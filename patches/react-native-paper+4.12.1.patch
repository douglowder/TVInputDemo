+  expect(() => getByText(affixTextValue)).not.toThrow();
+  expect(() => getByTestId('left-icon-adornment')).not.toThrow();
+  expect(() => getByTestId('right-affix-adornment')).not.toThrow();
+  expect(toJSON()).toMatchSnapshot();
+});
