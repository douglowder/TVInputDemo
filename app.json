{
  "expo": {
    "plugins": [
      "@react-native-tvos/config-tv",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false,
            "flipper": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ]
    ],
    "name": "TVInputDemo",
    "slug": "TVInputDemo",
    "android": {
      "package": "com.douglowderexpo.TVInputDemo",
      "userInterfaceStyle": "automatic"
    },
    "ios": {
      "bundleIdentifier": "com.douglowderexpo.TVInputDemo",
      "userInterfaceStyle": "automatic"
    }
  }
}
