{
  "cli": {
    "version": ">= 0.60.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "previewphone": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "scheme": "TVInputDemo"
      },
      "distribution": "internal"
    },
    "previewtv": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "scheme": "TVInputDemo-tvOS"
      },
      "distribution": "internal"
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}
