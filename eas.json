{
  "cli": {
    "version": ">= 0.60.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "previewphone": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "scheme": "TVInputDemo"
      },
      "distribution": "internal"
    },
    "previewtv": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "scheme": "TVInputDemo-tvOS"
      },
      "distribution": "internal"
    },
    "previewtvsim": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "scheme": "TVInputDemo-tvOS"
      },
      "distribution": "internal"
    },
    "production": {
      "ios": {
        "scheme": "TVInputDemo-tvOS"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6443614908",
        "bundleIdentifier": "com.douglowderexpo.TVInputDemo-tvOS"
      }
    }
  }
}
